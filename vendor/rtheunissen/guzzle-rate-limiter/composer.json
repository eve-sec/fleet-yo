{
    "name": "rtheunissen/guzzle-rate-limiter",
    "description": "Guzzle 6 middleware used to delay requests dynamically",
    "keywords": ["guzzle", "http", "middleware", "throttle", "rate", "psr7"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rudi Theunissen",
            "email": "rudolf.theunissen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Concat\\Http\\Middleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Concat\\Http\\Middleware\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
